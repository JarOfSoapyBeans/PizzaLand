{
	"version": 2,
	"buildCommand": "npm run vercel-build",
	"outputDirectory": "public",
	"rewrites": [
		{ "source": "/api/(.*)", "destination": "/api/$1.js" },
		{ "source": "/(.*)", "destination": "/public/$1" }
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600, s-maxage=86400"
				}
			]
		}
	]
}
